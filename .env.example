# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/sides_hub_dev

# Session Security
SESSION_SECRET=your-super-secret-session-key-at-least-32-characters

# Application Environment (local=local dev, dev=replit dev, prod=replit prod)
APP_ENV=local

# Replit Authentication (only required for APP_ENV=dev/prod)
# REPLIT_DOMAINS=your-replit-domain.replit.dev
# REPL_ID=your-repl-id
NODE_ENV=development

# Optional: Google Cloud Storage (for file uploads)
# GOOGLE_CLOUD_BUCKET_NAME=your-bucket-name
# GOOGLE_CLOUD_PROJECT_ID=your-project-id
PORT=3001
